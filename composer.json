{
    "name": "danielebarbaro/laravel-user-details",
    "description": "This package provide a list of common fields for a User entity",
    "keywords": [
        "danielebarbaro",
        "laravel",
        "laravel-user-details"
    ],
    "homepage": "https://github.com/danielebarbaro/laravel-user-details",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele Barbaro",
            "email": "barbaro.daniele@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Danielebarbaro\\UserDetail\\": "src",
            "Danielebarbaro\\UserDetail\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danielebarbaro\\UserDetail\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danielebarbaro\\UserDetail\\UserDetailServiceProvider"
            ],
            "aliases": {
                "UserDetail": "Danielebarbaro\\UserDetail\\Facades\\UserDetail"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
